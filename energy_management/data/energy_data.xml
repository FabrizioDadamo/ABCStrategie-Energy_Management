<odoo>
    <record id="ir_cron_check_consumption" model="ir.cron">
        <field name="name">Verifica Consumi Energetici</field>
        <field name="model_id" ref="model_energy_consumption"/>
        <field name="state">code</field>
        <field name="code">model.check_high_consumption()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
    </record>

    <record id="ir_cron_prediction_update" model="ir.cron">
        <field name="name">Aggiornamento Previsioni Energetiche</field>
        <field name="model_id" ref="model_energy_consumption"/>
        <field name="state">code</field>
        <field name="code">model._compute_prediction()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field> <!-- Frequenza dell'aggiornamento -->
        <field name="active">True</field>
    </record>




